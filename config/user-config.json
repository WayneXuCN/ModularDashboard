{
  "version": "0.1.0",
  "theme": "light",
  "layout": {
    "columns": 3,
    "width": "default",
    "show_nav": true,
    "center_content": false,
    "column_config": [
      {
        "width": "narrow",
        "modules": ["arxiv"]
      },
      {
        "width": "normal",
        "modules": ["github"]
      },
      {
        "width": "narrow",
        "modules": ["rss"]
      }
    ]
  },
  "modules": [
    {
      "id": "arxiv",
      "enabled": true,
      "collapsed": false,
      "config": {
        "refresh_interval": 3600,
        "keywords": [
          "machine learning",
          "artificial intelligence",
          "quantum computing"
        ]
      }
    },
    {
      "id": "github",
      "enabled": true,
      "collapsed": false,
      "config": {
        "refresh_interval": 3600,
        "username": "wayneXuCN",
        "repositories": [
          "ResearchDashboard",
          "nicegui"
        ]
      }
    },
    {
      "id": "rss",
      "enabled": true,
      "collapsed": false,
      "config": {
        "refresh_interval": 3600,
        "feed_urls": [
          "https://sspai.com/feed",
          "https://waynexucn.github.io/feed.xml"
        ],
        "fetch_limit": 10,
        "show_limit": 5,
        "show_author": true,
        "show_description": true,
        "show_date": true,
        "show_image": true
      }
    }
  ]
}